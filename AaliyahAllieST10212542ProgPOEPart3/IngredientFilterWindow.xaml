<Window x:Class="AaliyahAllieST10212542ProgPOEPart3.IngredientFilterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AaliyahAllieST10212542ProgPOEPart3"
        mc:Ignorable="d"
        Title="IngredientFilterWindow" Height="200" Width="300">
    <Grid>
        <StackPanel Margin="20">
            <Label Content="Enter Ingredient:" FontWeight="Bold"/>
            <TextBox x:Name="txtIngredient" Margin="0 5"/>
            <Button Content="Enter" HorizontalAlignment="Center" Margin="0 10"/>
        </StackPanel>
    </Grid>
</Window>
